<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <!-- Required elements-->
    <id>Fody.StandAlone</id>
    <description>Tool to IL weave .Net dlls without compiling them</description>
    <authors>Marcello Faga, Laurent Nguyen</authors>
    <projectUrl>https://github.com/AAATrafic/Fody.StandAlone</projectUrl>

    <!-- Optional elements -->
    <!-- ... -->
  </metadata>
  <!-- Optional 'files' node -->
  <files>
    <file src="bin\$configuration$\*.pdb" target="tools" />
    <file src="bin\$configuration$\*.dll" target="tools" />
    <file src="bin\$configuration$\*.exe" target="tools" />
  </files>
</package>
